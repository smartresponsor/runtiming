<?php
    declare(strict_types=1);

    // Example patch for Symfony 8 + FrankenPHP worker runtime.
    // Apply conceptually to your public/index.php.

    use App\Kernel;
    use Symfony\Component\Runtime\FrankenPhpSymfony\Runtime;

    require dirname(__DIR__).'/vendor/autoload.php';

    // NOTE: In worker-mode the Runtime will keep Kernel hot across requests.
    return function (array $context) {
        return new Kernel($context['APP_ENV'], (bool) $context['APP_DEBUG']);
    };
